+   {{ "unknown finger", "uf", NULL },	            1, {{num:0}}},            /*7*/
+   {{ "right thumb", "rt", NULL },                  2, {{num:1}, {num:11}}},      /*8*/
+   {{ "right index finger", "rif", "ri", NULL },    1, {{num:2}}},            /*9*/
+   {{ "right middle finger", "rmf", NULL },         1, {{num:3}}},           /*10*/
