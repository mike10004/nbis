+#define DAT_ID            5
+#define PRY_ID            6
+#define DAI_ID            7
+#define ORI_ID            8
+#define TCN_ID            9
+#define TCR_ID           10
+#define NSR_ID           11
+#define NTR_ID           12
+#define DOM_ID           13
+#define GMT_ID           14
+#define DCS_ID           15
+
+#define IDC_FMT           "%02d"
+#define FLD_FMT           "%d.%03d:"
+#define ASCII_CSID        0
