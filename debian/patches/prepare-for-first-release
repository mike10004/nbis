+#define MIN_QUALITY_VALUE             0
+/* Maximum Minutia Quality value. */
+#define MAX_QUALITY_VALUE            63
+
+/* Minimum scanning resolution in pixels/mm (500 dpi). */
+#define MIN_RESOLUTION               19.69
